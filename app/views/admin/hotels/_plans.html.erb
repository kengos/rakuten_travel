<h3><%= t('activerecord.models.plan') %></h3>
<div class="table-responsive">
  <table class="table table-bordered table-condensed">
    <thead>
      <th><%= Plan.human_attribute_name(:code) %></th>
      <th><%= Plan.human_attribute_name(:name) %></th>
      <th></th>
    </thead>
    <tbody>
      <% plans.each do |plan| -%>
        <tr>
          <td><%= plan.code %></td>
          <td><%= link_to plan.name, edit_admin_plan_path(plan) %></td>
          <td>
            <%= plan.payment_icon %>
            <%= plan.point_icon %>
            <%= plan.gift_icon %>
            <%= plan.breakfast_icon %><%= plan.dinner_icon %>
          </td>
        </tr>
      <% end -%>
    </tbody>
  </table>
</div>